@model List<WebApplication1.Models.movie>

@{
    ViewBag.Title = "MovieView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2> movieview</h2>
<h3> @Html.ActionLink("New Movie","Create1","Movie")</h3>
<table class="table table-bordered" table- hover'>
    <tr>
        <th>
            id
        </th>
        <th> moviename</th>
       
      
    </tr>
    @foreach (var m in Model)
    {
        <tr>
            <td> @m.id</td>
           
            <td> @Html.ActionLink(m.MovieName, "Details", "Movie", new { id = m.id }, null)</td>
            <td>
                @Html.ActionLink("Edit","EditMovie","Movie")</td>

      </tr>
  }
</table>





